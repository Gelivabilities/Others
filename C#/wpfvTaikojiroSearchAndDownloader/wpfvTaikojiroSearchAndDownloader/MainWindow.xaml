        <RadioButton Content="整合" HorizontalAlignment="Left" Height="15" Margin="10,44,0,0" VerticalAlignment="Top" Width="82" Checked="RadioButton_Checked_1"/>
        <RadioButton Content="单个" HorizontalAlignment="Left" Height="15" Margin="112,44,0,0" VerticalAlignment="Top" Width="64" Checked="RadioButton_Checked_2"/>
        <RadioButton Content="相关下载" HorizontalAlignment="Left" Height="15" Margin="196,44,0,0" VerticalAlignment="Top" Width="66" Checked="RadioButton_Checked_3"/>
        <Button Content="下载选中" HorizontalAlignment="Left" Height="35" Margin="10,375,0,0" VerticalAlignment="Top" Width="252" Click="Button_Click_1"/>

    </Grid>
</Window>
