t = Int(Rnd * 2) + 1
If t Mod 2 = 0 Then
Form1.Image1(i).Picture = LoadPicture(App.Path & "\image\dong.gif")

Else
Form1.Image1(i).Picture = LoadPicture(App.Path & "\image\ka.gif")
End If

End Function


